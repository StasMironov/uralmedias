								source(media="(max-width: 768px)" srcset="img/projects/magmetall-728.jpg")
								source(media="(min-width: 769px)" srcset="img/projects/magmetall-1260.jpg")
								img(src="img/projects/magmetall-1260.jpg" alt="mm" title="mm")
