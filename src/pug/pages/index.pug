extends ../layout/main

block variables
	- var title = 'Главная страница'
	- var pageClass = 'index-page';

	- var quote = 'h2';
	- var quoteTitle = 'Лучшие работы';
	- var quoteInfo = ['Наша компания с 2011 года реализовала более 300 проектов для бизнеса на основе данных и здравого смысла!'];

block content
	div(data-barba="container" data-barba-namespace="page")
		.bildboard.bildboard--main
			.bildboard__container.container
				.bildboard__text Магнитогорское Digital-агентство 
				h1.bildboard__title.title-1 Разработка<br> и продвижение
				.bildboard__quote.
					Мы проектируем и создаем <br/> <a href="corporate.html" class="sad-link">корпоративные сайты</a>,
					<a href="magazine.html" class="sad-link">интернет-магазины</a>,
					<a href="app.html" class="sad-link">мобильные приложения</a>
					<div class="bildboard__more">и многое другое</div>
				.rating
					.rating__title Рейтинг Рунета по Уралу 2020
					.rating__text 32 место по разработке сайтов
					.rating__text 34 место по продвижению сайтов
			video(autoplay="true" loop="loop" muted="muted" poster="media/video.mp4" data-parallax="scroll" data-image-src="media/video.mp4").bildboard__video
				<source src="media/video.mp4" type="video/mp4">
		.fill-background
			.portfolio(data-color="#fff")
				include ../modules/quote
				.portfolio__container.container
					.portfolio__items
						.portfolio__item.with-parallax(data-color="#0E4F95")
							picture
